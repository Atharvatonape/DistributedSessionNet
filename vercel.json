{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "includeFiles": [
      "utils/**",
      "load_balancing.py"
    ],
    "env": {
      "FLASK_ENV": "production"
    }
}
